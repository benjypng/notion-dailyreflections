    "html-to-text": "^9.0.5"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
