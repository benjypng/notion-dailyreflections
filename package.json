        "@semantic-release/commit-analyzer"
      ]
    ]
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "axios": "^1.7.2",
    "cron": "^3.1.7",
    "dayjs": "^1.11.11",
